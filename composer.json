{
    "name": "pr0ggy/kase",
    "description": "A lightweight unit testing framework for PHP",
    "version": "0.2.1",
    "type": "library",
    "bin": ["./bin/kase"],
    "keywords": ["php", "unit", "testing", "framework"],
    "license": "GPL",
    "require": {
        "php": ">=5.6.0",
        "symfony/console": "2.*",
        "pr0ggy/mockleton": "^0.3.0",
        "equip/structure": "^1.1",
        "sebastian/comparator": "^1.2"
    },
    "require-dev": {
        "pr0ggy/nark": "^0.2.0",
        "phpunit/phpunit": "^5.7",
        "hamcrest/hamcrest-php": "^2.0",
        "phpdocumentor/phpdocumentor": "2.*",
        "nette/finder": "^2.4"
    },
    "autoload": {
        "psr-4" : {
            "Kase\\" : "src/"
        },
        "files": [
            "src/const.php",
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4" : {
            "Kase\\Test\\Utils\\" : "tests/Utils/"
        },
        "files": []
    }
}
